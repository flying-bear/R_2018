library(tidyverse)
df1 <- read_csv('https://raw.githubusercontent.com/agricolamz/r_on_line_course_data/master/phoible_ejectives.csv')
fit1.1 <- dlm(data = df1, family = 'binomial', formula = have_ejectives~consonants)
fit1.1 <- glm(data = df1, family = 'binomial', formula = have_ejectives~consonants)
fit1.1 <- glm(data = df1, family = 'binomial', formula = have_ejectives~total)
fit1.2 <- glm(data = df1, family = 'binomial', formula = have_ejectives~total+area)
AIC(fit1.1, fit1.2)
predict(fit1.1, newdata = data.frame(total = 40))
predict(fit1.1, type = 'response', newdata = data.frame(total = 40), )
predict(fit1.1, type = 'response', newdata = data.frame(total = 40))
